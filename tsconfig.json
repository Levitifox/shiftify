{
    "include": ["src/**/*"],
    "compilerOptions": {
        "target": "ES2021",
        "module": "CommonJS",
        "moduleResolution": "node",
        "jsx": "react-jsx",
        "resolveJsonModule": true,
        "strict": true,
        "exactOptionalPropertyTypes": true,
        "noImplicitOverride": true,
        "noImplicitReturns": true
    }
}
